@model IEnumerable<OnlineBanking.Models.Account>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}

<div id="content-wrapper">
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">Tables</li>
        </ol>
        <!-- DataTables Example -->
        <div class="col-10">
            <h2>Account Number List</h2>
            <table class="table table-striped">
                <thead>
                    <tr><th>AccountNumber</th> 
                    <th>CreatedDate</th></tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>
                          <a href="@Url.Action("Balance", "Account", new { id = item.AccountNumber })"> @Html.DisplayFor(modelItem => item.AccountNumber)</a>
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreatedDate)
                        </td>
                    </tr>

                    }



                @*<tr>
        <td>ACC09435762</td>
    </tr>*@

                </tbody>
            </table>
      
            </div>
    </div>
    <!-- /.container-fluid -->
 </div> 
<!-- /.content-wrapper -->
<!-- /#wrapper -->
<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
</a>